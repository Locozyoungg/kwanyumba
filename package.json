{
  "name": "kwanyumba",
  "version": "1.0.0",
  "description": "Kwanyumba - A rental booking platform with M-Pesa integration",
  "type": "module",
  "main": "backend/src/server.js",
  "scripts": {
    "start": "node backend/src/server.js",
    "dev": "nodemon backend/src/server.js",
